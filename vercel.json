{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python@3.1.31",
            "config": {
                "maxLambdaSize": "50mb",
                "runtime": "python3.9",
                "installCommand": "pip install --no-cache-dir -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
} 